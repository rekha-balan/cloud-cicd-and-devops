[server]
35.189.6.29

# if you want to provide variables,
# you can do something like /etc/ansible/group_vars/server
# or /etc/ansible/host_vars/mainhost
# can optionally end in .yml or .yaml 


# another example includes: 
# server ansible_port=80 ansible_host=192.168.1.2

# Ex 1: Ungrouped hosts, specify before any group headers.
# Thre are two default groups: all and ungrouped
#green.example.com
#blue.example.com
#192.168.100.1
#192.168.100.10

# Ex 2: A collection of hosts belonging to the 'webservers' group
#[webservers]
#alpha.example.org
#beta.example.org
#192.168.1.100
#192.168.1.110

# If you have multiple hosts following a pattern you can specify
# them like this:
#www[001:006].example.com

# Ex 3: A collection of database servers in the 'dbservers' group
#[dbservers]
#
#db01.intranet.mydomain.net
#db02.intranet.mydomain.net
#10.25.1.56
#10.25.1.57

# Here's another example of host ranges, this time there are no
# leading 0s:
#db-[99:101]-node.example.com

